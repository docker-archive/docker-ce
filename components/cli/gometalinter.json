{
  "Vendor": true,
  "Deadline": "2m",
  "Sort": ["linter", "severity", "path", "line"],
  "Exclude": [
      "cli/compose/schema/bindata.go",
      "parameter .* always receives"
  ],
  "EnableGC": true,

  "DisableAll": true,
  "Enable": [
    "deadcode",
    "gocyclo",
    "gofmt",
    "goimports",
    "golint",
    "gosimple",
    "ineffassign",
    "interfacer",
    "lll",
    "misspell",
    "unconvert",
    "unparam",
    "unused",
    "vet"
  ],

  "Cyclo": 19,
  "LineLength": 200
}
